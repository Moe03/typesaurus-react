{
  "name": "@typesaurus/react",
  "version": "6.0.0-rc.2",
  "description": "React Hooks for Typesaurus, type-safe Firestore ODM",
  "keywords": [
    "React",
    "React Hooks",
    "Firebase",
    "Firestore",
    "TypeScript",
    "Typesaurus"
  ],
  "sideEffects": false,
  "main": "index.js",
  "module": "index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./index.d.mts",
        "default": "./index.mjs"
      }
    },
    "./useRead": {
      "require": {
        "types": "./useRead/index.d.ts",
        "default": "./useRead/index.js"
      },
      "import": {
        "types": "./useRead/index.d.mts",
        "default": "./useRead/index.mjs"
      }
    }
  },
  "repository": "https://github.com/kossnocorp/typesaurus-react",
  "author": "Sasha Koss <koss@nocorp.me>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "babel-plugin-add-import-extension": "^1.6.0",
    "preact": "^10.19.3",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "tsx": "^4.7.0",
    "typesaurus": "^10.0.0-rc.8",
    "typescript": "^5.3.3"
  }
}
